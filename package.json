{
  "name": "listed",
  "version": "1.0.1",
  "description": "Predictive shopping list",
  "main": "index.js",
  "scripts": {
    "test": "mocha",
    "clean": "mkdir -p dist/js && rm -r dist/*",
    "cp:moment": "cp node_modules/moment/min/moment.min.js dist/js/",
    "cp:localforage": "cp node_modules/localforage/dist/localforage.min.js dist/js/",
    "cp:vue": "cp node_modules/vue/dist/vue.min.js dist/js/",
    "cp:src": "cp -r source/* dist/",
    "build": "npm run clean && npm run cp:src && npm run cp:vue && npm run cp:localforage && npm run cp:moment",
    "link:vue": "cp node_modules/vue/dist/vue.min.js source/js/",
    "link:localforage": "cp node_modules/localforage/dist/localforage.min.js source/js/",
    "link:moment": "cp node_modules/moment/min/moment.min.js source/js/",
    "link": "npm run link:vue && npm run link:localforage && npm run link:moment"
  },
  "author": "Wesley Werner",
  "license": "GPL-3.0",
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.2.0"
  },
  "dependencies": {
    "localforage": "^1.4.3",
    "moment": "^2.17.1",
    "vue": "^2.1.10"
  }
}
